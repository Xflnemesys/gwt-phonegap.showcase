<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">
	
	<mgwt:panel.flex.RootFlexPanel>
	    <mgwt:header.HeaderPanel>
			<mgwt:button.image.PreviousitemImageButton ui:field="backButton" text="Modules"/>
			<mgwt:panel.flex.FlexSpacer/>
			<mgwt:header.HeaderTitle>Camera</mgwt:header.HeaderTitle>
			<mgwt:panel.flex.FlexSpacer/>
			<mgwt:panel.flex.FixedSpacer/>
		</mgwt:header.HeaderPanel>
		
		<mgwt:panel.scroll.ScrollPanel ui:field="scrollPanel">
			<g:Image ui:field="image"></g:Image>
		</mgwt:panel.scroll.ScrollPanel>
		
		<mgwt:buttonbar.ButtonBar>
			<mgwt:button.image.CameraImageButton ui:field="cameraButton"/>
		</mgwt:buttonbar.ButtonBar>

	</mgwt:panel.flex.RootFlexPanel>
</ui:UiBinder> 