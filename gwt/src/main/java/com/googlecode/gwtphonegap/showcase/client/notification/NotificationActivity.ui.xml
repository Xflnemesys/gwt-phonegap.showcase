<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">
	
	<mgwt:panel.flex.RootFlexPanel>
		<mgwt:header.HeaderPanel>
			<mgwt:button.image.PreviousitemImageButton ui:field="backButton" text="Modules"/>
			<mgwt:panel.flex.FlexSpacer/>
			<mgwt:header.HeaderTitle>Notification</mgwt:header.HeaderTitle>
			<mgwt:panel.flex.FlexSpacer/>
			<mgwt:panel.flex.FixedSpacer/>
		</mgwt:header.HeaderPanel>
		<mgwt:panel.scroll.ScrollPanel scrollingEnabledX="false">
			<g:FlowPanel>
				<mgwt:button.Button ui:field="alert" text="Alert"></mgwt:button.Button>		
				<mgwt:button.Button ui:field="confirm" text="Confirm"></mgwt:button.Button>		
				<mgwt:button.Button ui:field="vibrate" text="Vibrate"></mgwt:button.Button>		
				<mgwt:button.Button ui:field="beep" text="Beep"></mgwt:button.Button>		
			</g:FlowPanel>
		</mgwt:panel.scroll.ScrollPanel>
	</mgwt:panel.flex.RootFlexPanel>
</ui:UiBinder> 